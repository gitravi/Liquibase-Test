<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
		
		<changeSet id="001-senr-29Jul2015" author="senr" failOnError="false" runOnChange="true">
			
			<createProcedure catalogName="Test2"
				comments="Stored procedure test"
				dbms="SQL Server"
				encoding="utf8"
				path="testHello.sql"
				procedureName="testHello"
				relativeToChangelogFile="true"
				schemaName="public">
				CREATE OR REPLACE PROCEDURE testHello
				IS
				BEGIN
				  PRINT('Hello From The Database!');
				END;
			</createProcedure>
			<rollback>
			 <sql>
				DROP PROCEDURE testHello;
			 </sql>
			</rollback>
	</changeSet>
</databaseChangeLog>